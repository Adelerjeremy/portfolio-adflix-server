{
    "version": 2,
    "name": "nodejs-express",
    "builds": [
        {   "src": "/index.js", 
            "use": "@now/node-server",
            "config": { 
                "maxLambdaSize": "50mb"
                 }}
    ],
    "routes": [
        { "src": "/(.*)", "dest": "index.js" }
    ]
}